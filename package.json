{
  "name": "icolors-vars",
  "version": "0.0.7",
  "description": "System-level Color System Css Variables, fast and happy to use it",
  "main": "./antd.css",
  "type": "module",
  "scripts": {
    "gn:runtime": "tsc src/main.colors.ts",
    "gn:antd": "esno src/antd/index.ts",
    "gn:elm-plus": "esno src/elm-plus/index.ts",
    "gn:tailwind": "esno src/tailwind/index.ts",
    "gn:mui": "esno src/material-ui/index.ts",
    "dev": "npm run gn:antd && npm run gn:elm-plus && npm run gn:tailwind && npm run gn:mui",
    "gulp": "gulp",
    "rollup": "rollup -w -c",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx .",
    "lint:fix": "eslint --ext .ts,.js,.tsx,.jsx . --fix",
    "stylelint": "stylelint ./src/**/*.{css,less,scss,sass}",
    "stylelint:fix": "stylelint ./src/**/*.{css,less,scss,sass} --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "postinstallmac": "git config core.hooksPath .husky && chmod 777 .husky/*",
    "commit": "git add . && cz"
  },
  "keywords": [
    "css",
    "system",
    "system-css",
    "antd",
    "elm",
    "elm-plus",
    "tailwind",
    "material-ui"
  ],
  "author": "magesium",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "18.8.1",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-csso": "^4.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-prettier": "2.0.0",
    "ts-node": "10.9.1",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "mui.css",
    "antd.css",
    "elm-plus.css",
    "tailwind.css",
    "colors.js",
    "antd.colors.js",
    "elm-plus.colors.js",
    "mui.colors.js",
    "tailwind.colors.js",
    "README.md",
    "README-CN.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yyong008/iColorsSystem.git",
    "branch": "master",
    "platform": "github"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^3.10.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.34.1"
  }
}