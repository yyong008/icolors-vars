{
  "name": "icolors-vars",
  "version": "0.0.9",
  "description": "System-level Color System Css Variables, fast and happy to use it",
  "main": "./antd.css",
  "type": "module",
  "scripts": {
    "gn:dev": "cross-env NODE_ENV='development' esno ./src/run.ts",
    "gn:prod": "cross-env NODE_ENV='production' esno ./src/run.ts",
    "gn:utils": "tsup",
    "test": "vitest",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx .",
    "lint:fix": "eslint --ext .ts,.js,.tsx,.jsx . --fix",
    "stylelint": "stylelint ./src/**/*.{css,less,scss,sass}",
    "stylelint:fix": "stylelint ./src/**/*.{css,less,scss,sass} --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "postinstallmac": "git config core.hooksPath .husky && chmod 777 .husky/*",
    "commit": "git add . && cz",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "css",
    "system",
    "system-css",
    "antd",
    "elm",
    "elm-plus",
    "tailwind",
    "material-ui"
  ],
  "author": "magesium",
  "license": "MIT",
  "devDependencies": {
    "@algolia/client-search": "4.9.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.0",
    "@types/node": "18.8.1",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-prettier": "2.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "vite": "^4.2.1",
    "vitepress": "1.0.0-alpha.50",
    "vitest": "^0.29.7",
    "vue": "^3.2.47"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "index.js",
    "antd.css",
    "antd.js",
    "elm-plus.css",
    "elm-plus.js",
    "mui.css",
    "mui.js",
    "tailwind.css",
    "tailwind.js",
    "README.md",
    "README-CN.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yyong008/iColorsSystem.git",
    "branch": "master",
    "platform": "github"
  },
  "dependencies": {
    "iesmo": "^0.0.2",
    "tsup": "^6.7.0"
  }
}
